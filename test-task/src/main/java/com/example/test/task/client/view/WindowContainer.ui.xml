<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel stylePrimaryName="windowPanel">
		<g:HorizontalPanel stylePrimaryName="windowHeaderPanel">
			<g:SimplePanel ui:field="windowIconPanel" />
			<g:Label text="Please set window title here" ui:field="titleLbl" />
			<g:PushButton ui:field="closeBtn" stylePrimaryName="windowCloseButton">
			</g:PushButton>
		</g:HorizontalPanel>
		<g:SimplePanel ui:field="contentPanel" stylePrimaryName="windowContentPanel" />
		<g:HorizontalPanel ui:field="controlPanel"
			horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE" stylePrimaryName="windowControlPanel" />
	</g:VerticalPanel>
</ui:UiBinder> 